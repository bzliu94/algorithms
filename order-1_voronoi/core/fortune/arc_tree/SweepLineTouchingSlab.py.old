from BeachLineItem import *

class SweepLineTouchingSlab(BeachLineItem):

  def __init__(self, left_x_value, right_x_value, sweep_line):

    self.left_x_value = left_x_value

    self.right_x_value = right_x_value

    self.sweep_line = sweep_line

  def setLeftXValue(self, left_x_value):

    self.left_x_value = left_x_value

  def setRightXValue(self, right_x_value):

    self.right_x_value = right_x_value

  def setSweepLine(self, sweep_line):

    self.sweep_line = sweep_line

  def getLeftXValue(self):

    return self.left_x_value

  def getRightXValue(self):

    return self.right_x_value

  def getSweepLine(self):

    return self.sweep_line

  def getLowYValue(self):

    return self.getSweepLine().getY()

  def isSweepLineTouchingSlab(self):

    return True

