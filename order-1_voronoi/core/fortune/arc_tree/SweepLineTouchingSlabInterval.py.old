from Interval import *

from SweepLineTouchingSlab import *

class SweepLineTouchingSlabInterval(Interval):

  def __init__(self, slab):

    Interval.__init__(self, None, None)

    self.slab = slab

  def getSlab(self):

    return self.slab

  def getLeftEndpoint(self):

    return self.getSlab().getLeftXValue()

  def getRightEndpoint(self):

    return self.getSlab().getRightXValue()

  def setSlab(self, slab):

    self.slab = slab

