from IntervalTreeNode import *

class OrderedIntervalTreeNode(IntervalTreeNode):

  def __init__(self, element, parent, left_child, right_child, max = None, prev = None, next = None):
  
    IntervalTreeNode.__init__(self, element, parent, left_child, right_child, max)
    
    self.prev = prev

    self.next = next

  def getPrev(self):

    return self.prev

  def getNext(self):

    return self.next

  def setPrev(self, prev):

    self.prev = prev

  def setNext(self, next):

    self.next = next

